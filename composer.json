        "symfony\/routing": ">=v2.1.7",
        "symfony\/console": ">=v2.1",
        "marijnh\/codemirror": ">=v3.1",
        "sybio\/image-workshop": "dev-master",
        "tinymce\/tinymce": "*",
        "propel\/propel": "dev-master",
        "composer\/composer": "*",
        "kryncms\/demotheme-bundle": "*",
        "google\/closure-compiler": "*",
        "symfony\/filesystem": "2.2.*",
        "phpunit\/phpunit": "3.7.*",
        "kryncms\/publication-bundle": "*",
        "michelf/php-markdown": "1.3"
    }
}