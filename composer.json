{
    "repositories": [
        {
            "type": "composer",
            "url": "http:\/\/packages.kryn.org\/"
        }
    ],
    "name": "kryncms\/kryn.cms",
    "description": "A open-source enterprise Content-Management-System CMS and Content-Management-Framework CMF based on Propel and Symfony components.",
    "keywords": [
        "CMS",
        "PHP CMS",
        "WCMS"
    ],
    "license": "GPL",
    "homepage": "http:\/\/www.kryn.org\/",
    "authors": [
        {
            "name": "Marc J. Schmidt",
            "homepage": "https:\/\/github.com\/marcj",
            "email": "marc@kryn.org"
        }
    ],
    "autoload": {
        "psr-0": {
            "Core": "src/",
            "Admin": "src/",
            "Users": "src/",
            "Test": "tests/bundles/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4",
        "twig\/twig": ">=1.12",
